<%=form_with model: @user, url: users_path, local: true, data: { turbo: false }  do |f| %>
<%=f.label :email%>
<%=f.text_field :email%>
<%=f.label :password%>
<%=f.password_field :password%>
<%=f.label :password_confirmation%>
<%=f.password_field :password_confirmation%>
<%=f.submit "Sign up"%>
<%end%>